{
  "name": "omogenjudge",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npm run css",
    "css-compile": "node-sass --include-path node_modules --output-style compressed --source-map true --source-map-contents true --precision 6 scss -o static/css/",
    "css-lint": "stylelint scss/",
    "css-prefix": "postcss --replace static/css/main.css --use autoprefixer --map",
    "css-purge": "purgecss --keyframes --css static/css/main.css --content index.html \\\\\\\"node_modules/bootstrap/js/dist/{util,modal}.js\\\\\\\" --output static/css/",
    "css": "npm-run-all css-compile css-prefix",
    "start": "npm-run-all --parallel watch server",
    "watch": "nodemon -e html,scss -x \"npm run css\"",
    "test": "npm run css-lint && npm run css"
  },
  "dependencies": {
    "bootstrap": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "purgecss": "^4.0.2",
    "serve": "^11.3.2",
    "stylelint": "^13.12.0",
    "stylelint-config-twbs-bootstrap": "^2.1.0"
  }
}
